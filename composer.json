{
    "name": "free-elephants/json-api-toolkit",
    "description": "Tools for building specification first json:api / swagger integrated APIs",
    "license": "BSD-2",
    "authors": [
        {
            "name": "samizdam",
            "email": "samizdam@inbox.ru"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "ext-json": "^7.4",
        "cebe/php-openapi": "^1.4",
        "free-elephants/di": "^2.0",
        "neomerx/json-api": "^4.0",
        "nette/php-generator": "^3.3",
        "nikic/fast-route": "^1.3",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.7",
        "nyholm/psr7": "^1.2",
        "phpunit/phpunit": "^9.0"
    },
    "suggest": {
        "doctrine/orm": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "FreeElephants\\": [
                "src/FreeElephants"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FreeElephants\\": [
                "tests/FreeElephants"
            ]
        }
    }
}
